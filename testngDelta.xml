<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="DeltaPages Tests Suite"  thread-count="1">

       <listeners>
            <listener class-name="utilities.TestListener"/>
        </listeners>

        <test name="Application SignUp Test" >
            <classes>
                <class name="DeltaTests.ApplicationSignUpTest"/>
            </classes>
        </test>

        <test name="Booking with Domestic Airlines Test" >
            <classes>
                <class name="DeltaTests.booking.Booking_DomesticAirlinesTest"/>
            </classes>
        </test>

        <test name="Finding my Trips" >
            <classes>
                <class name="DeltaTests.MyTripsTest"/>
            </classes>
        </test>

        <test name="CheckIn With ConfirmationNumber Test" >
            <classes>
                <class name="DeltaTests.checkIn.CheckInWithConfirmationNumberTest"/>
            </classes>
        </test>

        <test name="CheckIn With CreditCardNumberTest" >
            <classes>
                <class name="DeltaTests.checkIn.CheckInWithCreditCardNumberTest"/>
            </classes>
        </test>


</suite>
